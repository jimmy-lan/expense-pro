rails: sh -c '[ "$RAILS_ENV" = "production" ] && exit 0 || bundle exec rails server -p 3001 -b 127.0.0.1'
react: sh -c '[ "$NODE_ENV" = "production" ] && exit 0 || (cd client && PORT=5100 HOST=0.0.0.0 DANGEROUSLY_DISABLE_HOST_CHECK=true npm start)'
tunnel: sh -c '[ "$NODE_ENV" = "production" ] && exit 0 || (sleep 5 && cloudflared tunnel --url http://127.0.0.1:5100)'